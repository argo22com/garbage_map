{
  "extends": [
    "config:js-app",
    "group:jsTestMonMajor",
    "schedule:weekly"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true,
      "automergeType": "branch"
    }
  ],
  "prHourlyLimit": 0,
  "stabilityDays": 3,
  "masterIssue": true
}
